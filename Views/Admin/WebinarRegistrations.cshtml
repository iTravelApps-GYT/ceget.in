@using (Html.BeginForm("WebinarRegistrations", "Admin", FormMethod.Post, new { @enctype = "multipart/form-data" }))
{ 
<center><p>Webinar Registrations Management</p></center>
    <br />
    <center><table id="tblData" class="CSSTableGenerator">
        <tr>
            <td>Full Name</td>
            <td>Email ID</td>
            <td>Regitration Date</td>
            <td>Webinar Title</td>
            <td>Webinar Date</td>
            <td>Webinar Time</td>
            <td width="170" align="center">Action</td>
        </tr>
        @foreach (GCNI.Models.Registration obj in Model.objRegistration_ML)
               {
                <tr>
                    <td>
                       <span>@Html.Raw(obj.FullName)</span>
                    </td>
                    <td>
                       <span>@Html.Raw(obj.EmailID)</span>
                    </td>
                    <td>
                       <span>@Html.Raw(obj.RegistrationDate)</span>
                    </td>
                    <td>
                        <span>@Html.Raw(obj.WebinarTitle)</span>
                    </td>
                    <td>
                        <span>@Html.Raw(obj.WebinarDate)</span>
                    </td>
                    <td>
                        <span>@Html.Raw(obj.WebinarTime)</span>
                    </td>
                    <td>
                       <span><input id="Delete" class="testbutton1" type="submit" value="Delete" name="btntype" onclick="Deleterecord(@obj.id);"/></span>
                    </td>
                </tr>
               }
    </table></center><br /><br /><br /><br /><br />
    <input type="hidden" id="hdd" name="hdd" value="0" />
    <input type="hidden" id="id" name="id" value="0" />
    <input type="hidden" id="Vlnk" name="Vlnk" value="0" />
    <input type="hidden" id="file" name="file" value="0" />
}
