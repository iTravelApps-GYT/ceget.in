@model GCNI.Models.GCNIModel

@{
    ViewBag.Title = "WebinarRegistrations";
    Layout = "~/Views/Shared/AdminMaster.cshtml";
}
<link href="~/css/tableStyle.css" rel="stylesheet" />
<style>
     .tbtext {
	border:2px solid #456879;
	border-radius:10px;
	height: 30px;
	width: 500px;
    padding: 7px
}
.tbeditor {
	border:2px solid #456879;
	border-radius:10px;
	height: 100px;
