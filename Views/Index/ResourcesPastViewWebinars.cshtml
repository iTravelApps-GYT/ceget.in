    <div style="width:100%; display:none;margin-left: 200px;margin-top: -20px;" align="center" id="Registration">

        <div style="position: fixed;left: 50%;top: 50%;transform: translate(-50%, -50%); height: 250px;color:black; width: 500px; z-index: 1000; background-color: #586778;border:solid;">
            <a href="#" onclick="$('#Registration').hide(); return false;"><span style="float: right; color: white; font-weight: normal; font-size: larger; margin-right: 5px;">X</span></a>
            <div style="background-color: white; height: 205px; margin: 20px;padding: 1px;">
                <div class="register-box">
                    <div class="register-box-body">
                        <p class="login-box-msg" style="margin-left: 10px;"><b>Registration</b></p>
                        @using (Html.BeginForm("Registration", "Index", FormMethod.Post, new { @enctype = "multipart/form-data" }))
                        {
                            <div class="form-group has-feedback">
                                @Html.TextBoxFor(M => M.objRegistration_M.FullName, new { @id = "FullName", @type="text" , @class = "form-control", @placeholder = "Full name", @Maxlength = "50", @onblur = "msgclear();RemoveClass(this);" })
                                @*<input type="text" class="form-control" placeholder="Full name">*@
                                @*<span class="glyphicon glyphicon-user form-control-feedback"></span>*@
                            </div>
                            <div class="form-group has-feedback">
                                @Html.TextBoxFor(M => M.objRegistration_M.EmailID, new { @id = "EmailID", @type = "email" , @class = "form-control", @placeholder = "Email", @Maxlength = "50", @onblur = "msgclear();RemoveClass(this);" })
                                @*<input type="email" class="form-control" placeholder="Email">*@
                                @*<span class="glyphicon glyphicon-envelope form-control-feedback"></span>*@
                            </div>
                            <div class="row">
                                @*<div class="col-xs-8">
                                </div>*@<!-- /.col -->
                                <div class="col-xs-4">
                                    <button type="submit" id="regbtn" name="regbtn" value="0" class="btn btn-primary btn-block btn-flat">Register</button>
                                </div><!-- /.col -->
                            </div>
                        }
                    </div><!-- /.form-box -->
                </div><!-- /.register-box -->
            </div>
        </div>
    </div>
    <div style="width:100%; display:none;margin-left: 200px;margin-top: -20px;" align="center" id="REG">

