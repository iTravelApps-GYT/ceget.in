<script type="text/javascript">
    $(document).ready(function () {
        debugger;
        var myJsVariable = '@ViewBag.RegValue'
        //alert(myJsVariable);
        if (myJsVariable.length > 0 && myJsVariable == "Done") {
            $("#REG").show();
            $("#NREG").hide();
        }
        else if (myJsVariable.length > 0 && myJsVariable == "Not Done") {
            $("#REG").hide();
            $("#NREG").show();
        }
        else {
            $("#REG").hide();
            $("#NREG").hide();
        }
    });
</script>
